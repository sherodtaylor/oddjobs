<%= form_for(@oddjob) do |f| %>

  <div class="field">
   <%= f.label :job %>
   <%= f.text_field :job %>
  </div>
 
  
  <div class="field">
   <%= f.label :company %>
   <%= f.text_field :company %>
  </div>

  
  <div class="field">
   <%= f.label :description %>
   <%= f.text_field :description %>
  </div>
<div class="actions">
    <%= f.submit %>
  </div>
<% end %>